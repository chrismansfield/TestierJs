{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "linebreak-style": "off",
        "semi": "error",
        "quotes": ["error", "single"],
        "indent": [
            "error",
            4
        ],
        "padded-blocks": "off",
        "class-methods-use-this": "off"
    }
}