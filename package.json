  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-import-resolver-alias": "^1.0.3",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "2.7.0",
    "gulp": "^3.9.1",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-babel": "^1.0.1",
    "lint-staged": "^4.2.3"
  }
}
