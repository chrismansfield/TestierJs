{
  "name": "testier",
  "version": "0.0.0",
  "description": "Making your tests testier!",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "lint-fix-all": "eslint --fix \"src/**/*.js\" \"tests/**/*.js\"",
    "precommit": "lint-staged",
